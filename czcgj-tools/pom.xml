        <version>1.0.0</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>

    <artifactId>czcgj-tools</artifactId>

    <properties>
        <!--  MyBatis Generator  -->
        <!--  Java接口和实体类  -->
        <targetJavaProject>/Users/zhangyuan/Desktop/mlcz/src/main/java</targetJavaProject>
        <targetMapperPackage>com.sbq.mapper</targetMapperPackage>
        <targetModelPackage>com.sbq.model</targetModelPackage>
        <!--  XML生成路径  -->
        <targetResourcesProject>/Users/zhangyuan/Desktop/mlcz/src/main/java/com/sbq/mapping</targetResourcesProject>
        <targetXMLPackage>mapper</targetXMLPackage>
        <!--  依赖版本  -->
        <mysql.version>5.1.29</mysql.version>

    </properties>

    <dependencies>
        <dependency>
            <groupId>com.sbq</groupId>
            <artifactId>czcgj-common</artifactId>
